# Порт сервера
PORT=3001

# URL фронтенда для CORS
FRONTEND_URL=http://localhost:3000

# Секретный ключ для JWT токенов
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production

# Режим работы
NODE_ENV=development

# PostgreSQL настройки
DB_CLIENT=postgres
PGHOST=localhost
PGPORT=5432
PGDATABASE=magazin
PGUSER=postgres
PGPASSWORD=postgres

# Альтернативно можно использовать одну строку:
# POSTGRES_URL=postgres://postgres:postgres@localhost:5432/magazin


